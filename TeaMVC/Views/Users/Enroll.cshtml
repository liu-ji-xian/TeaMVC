
@{
    Layout = null;
}
@using TeaMVC.Models
@model List<Users>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>注册</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <link href="~/csss/reset.css" rel="stylesheet" />
    <link href="~/csss/common.css" rel="stylesheet" />
    <link href="~/csss/font-awesome.min.css" rel="stylesheet" />
</head>
<body>
    <div class="wrap login_wrap">
        <div class="content">

            <div class="logo"></div>

            <div class="login_box">

                <div class="login_form">
                    <div class="login_title">
                        注册
                    </div>
                    <form action="Enroll" method="post">

                        <div class="form_text_ipt">
                            <input name="UNo" type="text" placeholder="用户编号">
                        </div>
                        <div class="ececk_warning"><span>用户编号不能为空</span></div>
                        <div class="form_text_ipt">
                            <input name="UPass" type="password" placeholder="密码">
                        </div>
                        <div class="ececk_warning"><span>密码不能为空</span></div>
                        <div class="form_text_ipt">
                            <input name="UPass" type="password" placeholder="重复密码">
                        </div>
                        <div class="ececk_warning"><span>密码不能为空</span></div>

                        <div class="form_btn">
                            <button type="button" onclick="location='/Users/Login'" class="btn btn-info">注册</button>
                        </div>
                        <div class="form_reg_btn">
                            <span>已有帐号？</span><a href="/Users/Login">马上登录</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="js/jquery.min.js"></script>
    <script type="text/javascript" src="js/common.js"></script>
</body>
</html>
